// arr - input array
// n - size of array

int FindSortedArrayRotation(int arr[], int n) {
    /* Don't write main().
     * Don't read input, it is passed as function argument.
     * Return output and don't print it.
     * Taking input and printing output is handled automatically.
     */
  int i=0;
while(i<n-1)
{ 
  if(arr[i]<=arr[i+1])
  {
    i++;
  }
  else
  {
    return (i+1);
  }
}
  return 0;
}
